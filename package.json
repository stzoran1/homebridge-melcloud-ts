{
    "private": false,
    "displayName": "Homebridge MELCloud Zubadan",
    "name": "homebridge-melcloud-ts-zubadan",
    "version": "0.1.0",
    "description": "Homebridge plugin for MELCloud Zubadan (readonly).",
    "main": "dist/index.js",
    "files": [
        "dist/*.js",
        "dist/**/*.js",
        "config.schema.json"
    ],
    "scripts": {
        "clean": "rimraf ./dist ./coverage",
        "build": "rimraf ./dist ./coverage && tsc",
        "lint": "eslint src/*.ts --fix",
        "lint-check": "eslint src/*.ts",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint-check",
        "postpublish": "npm run clean",
        "publish-next": "npm publish . --tag next",
        "watch": "npm run clean && tsc --watch"
    },
    "engines": {
        "node": ">=10",
        "homebridge": ">=1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stzoran1/homebridge-melcloud-ts-zubadan.git"
    },
    "keywords": [
        "homebridge-plugin",
        "Mitsubishi",
        "Melcloud",
        "homekit",
        "Siri",
        "ts",
        "TypeScript",
        "Zubadan",
        "Ecodan"
    ],
    "author": "Dids",
    "contributors": [
        "stzoran1bg"
    ],
    "maintainers": [
        "stzoran1bg"
    ],
    "license": "GPL2",
    "bugs": {
        "url": "https://github.com/stzoran1/homebridge-melcloud-ts-zubadan/issues"
    },
    "homepage": "https://github.com/stzoran1/homebridge-melcloud-ts-zubadan",
    "dependencies": {
        "async-mutex": "^0.3.1",
        "lodash": "^4.17.21",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.1",
        "node-persist": "^3.1.0",
        "object-hash": "^2.2.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.170",
        "@types/node": "^15.12.2",
        "@types/node-cache": "^4.2.5",
        "@types/node-fetch": "^2.5.10",
        "@types/node-persist": "^3.1.1",
        "@types/object-hash": "^2.1.0",
        "@types/request-promise-native": "^1.0.17",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "eslint": "^7.28.0",
        "homebridge": "^1.3.4",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.2"
    }
}